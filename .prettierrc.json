{
    "experimentalTernaries": false,
    "experimentalOperatorPosition": "start",
    "printWidth": 9999,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "bracketSpacing": true,
    "objectWrap": "preserve",
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto",
    "singleAttributePerLine": false
}
